{
    "name": "indigophp/fuel-dependency",
    "type": "fuel-package",
    "description": "Provides a backport of v2 dependency library",
    "license": "MIT",
    "keywords": ["dependency", "facade"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "fuelphp/dependency": "dev-master",
        "indigophp/fuel-composer": "dev-develop"
    },
    "require-dev": {
        "codeception/codeception": "~2.0.0",
        "codeception/mockery-module": "dev-master",
        "indigophp/codeception-fuel-module": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Fuel\\Dependency\\": "classes/"
        }
    },
    "extra": {
        "installer-name": "dependency"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
